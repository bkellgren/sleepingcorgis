{
  "name": "Sleeping Corgis",
  "version": "1.4",
  "homepage_url": "http://bkellgren.github.io/sleepingcorgis/",
  "description": "Replaces all images on a given page with pictures of Sleeping Corgis!!",
  "icons": { "16"  : "img/icon_16.png",
             "48"  : "img/icon_48.png",
             "128" : "img/icon_128.png"},
  "browser_action": {
    "default_icon": "img/icon_48.png",
    "default_title": "SLEEPING CORGIS"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js", "sleepingcorgis.js"],
    "persistent": false
  },
  "manifest_version": 2
}
